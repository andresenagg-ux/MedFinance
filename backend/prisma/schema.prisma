generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Profile {
  STUDENT
  RECENT_GRAD
  SPECIALIST
}

model User {
  id        String   @id
  profile   Profile?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
